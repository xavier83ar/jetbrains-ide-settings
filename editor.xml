<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_DOC_COMMENTS" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="NEXT_ERROR_ACTION_GOES_TO_ERRORS_FIRST" value="false" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="TodoConfiguration">
    <pattern case-sensitive="true" pattern="\b@todo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern case-sensitive="true" pattern="@task" />
  </component>
  <component name="XmlEditorOptions">
    <option name="tagTreeHighlightingLevelCount" value="4" />
  </component>
</application>